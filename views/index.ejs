            <!-- SERVICES SECTION  -->
            <!-- <section class="services">
        <div class="container">
            <img src="/images/Services.png" alt="">
        </div>
    </section> -->
            <!-- SERVICES SECTION ENDS  -->

            <!-- CATEGORIES SECTION  -->
            <section class="categories">
                <h1 class="heading">CATEGORIES</h1>
                <div class="container">
                    <div class="category-container">
                        <a href="/Shop/Women">
                            <div class="categories-divs">
                                <img src="/images/StockCake-Urban Fashion Model_1722963883.jpg" width="100%"
                                    alt="women image">
                                <div class="img-text women-class">
                                    <% if (locals.totalWomenItems) { %>
                                        <% } %>
                                            <h3>Women</h3>
                                            <p>
                                                <%= totalWomenItems %> items
                                            </p>
                                </div>
                            </div>
                        </a>
                        <a href="/Shop/Men">
                            <div class="categories-divs">
                                <img src="/images/StockCake-Urban Denim Fashion_1722964470.jpg" width="100%"
                                    alt="men image">
                                <div class="img-text  men-class">
                                    <% if (locals.totalMenItems) { %>
                                        <% } %>
                                            <h3>Men</h3>
                                            <p>
                                                <%= totalMenItems %> items
                                            </p>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            </section>
            <!-- CATEGORIES SECTION ENDS  -->

            <!-- ADVERTISEMENT SECTION  -->
            <div class="container">
                <marquee behavior="scroll" direction="left">UPTO 50% OFF ON YOUR FIRST ORDER</marquee>
            </div>
            <!-- ADVERTISEMENT SECTION ENDS  -->

            <!-- ARRIVALS SECTION STARTS  -->
            <div class="splide">
                <h1 class="heading">NEW ARRIVALS</h1>
                <div class="splide__track">
                    <div class="splide__list">
                        <div class="splide__slide">
                            <div class="img_box">
                                <img src="/images/women_cargo01.webp" alt="">
                            </div>
                            <p>Top-Notch Tops</p>
                        </div>
                        <div class="splide__slide">
                            <div class="img_box">
                                <img src="/images/women_dresses1.webp" alt="">
                            </div>
                            <p>Top-Notch Tops</p>
                        </div>
                        <div class="splide__slide">
                            <div class="img_box">
                                <img src="/images/co-ord set2.webp" alt="">
                            </div>
                            <p>Top-Notch Tops</p>
                        </div>
                        <div class="splide__slide">
                            <div class="img_box">
                                <img src="/images/women cargo2.webp" alt="">
                            </div>
