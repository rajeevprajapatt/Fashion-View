                        </div>
                        <div class="splide__slide">
                            <div class="img_box">
                                <img src="/images/women cargo1.webp" alt="">
                            </div>
                            <p>Top-Notch Tops</p>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ARRIVALS SECTION END  -->

            <!-- NEWSLETTER SECTION  -->
            <h1 class="heading" style="margin-top: 3rem; margin-bottom: 0rem;">SUBSCRIBE FOR LATEST UPDATES</h1>
            <section class="newsletter">
                <div class="container">
                    <form action="">
                        <h3>Subscribe for latest updates</h3>
                        <input type="email" name="" placeholder="enter your email" id="" class="box">
                        <input type="submit" value="subscribe" class="btn">
                    </form>
                </div>
            </section>
            <!-- NEWSLETTER SECTION ENDS  -->

            <!-- HOMEGROWN SECTION  -->
            <%- include("./partials/homeGrownBranding") %>
                <!-- HOMEGROWN SECTION ENDS  -->

                <!-- FOOTER SECTION  -->
                <%- include("./partials/footer") %>
                    <!-- FOOTER SECTION ENDS  -->

                    <!-- COPYRIGHT SECTION  -->
                    <div class="copyright">
                        <p>&copy; 2023 Developed By Rajeev Prajapat</p>
                    </div>
                    <!-- COPYRIGHT SECTION ENDS  -->

                    <script src="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js"></script>
                    <script src="/js/splide.js"></script>

