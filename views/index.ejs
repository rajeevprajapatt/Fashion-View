<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Fashion View</title>
    <!-- custom css file link -->
    <link rel="stylesheet" href="/style.css">

    <link rel="shortcut icon" href="/images/cropped_image.png" type="image/x-icon">

    <!-- link font awesom cdn  -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
        integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css" />

    <script src="jquery-3.7.1.min.js"></script>
</head>

<body>
    <!-- HEADER SECTION -->
    <header class="header">
        <div class="header-1">
            <a href="#" class="logo"><i class="fa-brands fa-foursquare fa-beat"></i>ashi&#xf8;&#x014B; View</a>
            <form action="" class="search-form">
                <input type="search" name="" placeholder="Search here....." id="search-box">
                <label form="search-box" class="fas fa-search"></label>
            </form>
            <div class="icons">
                <div id="search-btn" class="fas fa-search"></div>
                <a href="#" class="fas fa-heart"></a>
                <a href="#" class="fas fa-shopping-cart"></a>
                <a href="/user/login" class="fas fa-user"></a>
            </div>
        </div>
    </header>
    <!-- HEADER SECTION ENDS -->

    <!-- SLIDESHOW SECTION  -->
    <section class="slideshow-container">
        <div class="container">
            <div class="mySlides fade">
                <a href="#"><img src="/images/homepage_banner_copy_6.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/homepage_86ual9W.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/Homepage-Banner_1_Vva0SiZ.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/homepage_banner_copy_2_dN8S3HR.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/Homepage-Banner_4_Ys7LZ9a.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/PINK_PANTHER_homepage_rW2gnAR.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/Homepage-banner_2.webp" style="width:100%"></a>
            </div>
            <div class="mySlides fade">
                <a href="#"><img src="/images/Homepage-Banner_batman_oversized-t-shirt_2.webp" style="width:100%"></a>
            </div>
        </div>
    </section>

    <section style="text-align:center">
        <div class="container">
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
            <span class="dot"></span>
        </div>
    </section>
    <!-- SLIDESHOW CONTAINER ENDS  -->

    <!-- SERVICES SECTION  -->
    <section class="services">
        <div class="container">
            <img src="/images/Services.png" alt="">
        </div>
    </section>
    <!-- SERVICES SECTION ENDS  -->

    <!-- CATEGORIES SECTION  -->
    <section class="categories">
        <h1 class="heading">CATEGORIES</h1>
        <div class="container">
            <div class="category-container">
                <a href="/Shop/Women">
                    <div class="categories-divs">
                        <img src="/images/pexels-anastasiya-gepp-654466-1462637.jpg" width="100%" alt="women image">
                        <div class="img-text women-class">
                            <% if (locals.totalwomen) { %>

                                <% } %>
                                    <h3>Women</h3>
                                    <p>
                                        <%= totalwomen %> items
                                    </p>
                        </div>
                    </div>
                </a>
                <a href="/Shop/Men">
                    <div class="categories-divs">
                        <img src="/images/pexels-yogendras31-3595231.jpg" width="100%" alt="men image">
                        <div class="img-text  men-class">
                            <% if (locals.totalmen) { %>

                                <% } %>
                                    <h3>Men</h3>
                                    <p>
                                        <%= totalmen %> items
                                    </p>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </section>
    <!-- CATEGORIES SECTION ENDS  -->

    <!-- ADVERTISEMENT SECTION  -->
    <div class="container">
        <marquee behavior="scroll" direction="left">UPTO 50% OFF ON YOUR FIRST ORDER</marquee>
    </div>
    <!-- ADVERTISEMENT SECTION ENDS  -->

    <!-- ARRIVALS SECTION  -->
    <section class="arrivals">
        <h1 class="heading">NEW ARRIVALS</h1>
        <div class="container">
            <div class="arrivals-div">
                <div class="arrival">
                    <img src="/images/women_cargo01.webp" alt="Top-Notch Tops">
                    <p>Top-Notch Tops</p>
                </div>
                <div class="arrival">
                    <img src="/images/women_dresses1.webp" alt="Dreamy Dresses">
                    <p>Dreamy Dresses</p>
                </div>
                <div class="arrival">
                    <img src="/images/co-ord set2.webp" alt="Chic Jumpsuits">
                    <p>Chic Jumpsuits</p>
                </div>
                <div class="arrival">
                    <img src="/images/women cargo2.webp" alt="Chic Jumpsuits">
                    <p>Chic Jumpsuits</p>
                </div>
            </div>
        </div>
    </section>
    <!-- ARRIVALS SECTION ENDS  -->

    <!-- NEWSLETTER SECTION  -->
    <h1 class="heading" style="margin-top: 3rem; margin-bottom: 0rem;">SUBSCRIBE FOR LATEST UPDATES</h1>
    <section class="newsletter">
        <div class="container">
            <form action="">
                <h3>subscribe for latest updates</h3>
                <input type="email" name="" placeholder="enter your email" id="" class="box">
                <input type="submit" value="subscribe" class="btn">
            </form>
        </div>
    </section>
    <!-- NEWSLETTER SECTION ENDS  -->

    <!-- HOMEGROWN SECTION  -->
    <section class="Homegrown">
        <div class="container">
            <div>
                <h5>HOMEGROWN INDIAN BRAND</h5>
            </div>
            <div>
                <h5>OVER 100K HAPPY CUSTOMERS</h5>
            </div>
        </div>
    </section>
    <!-- HOMEGROWN SECTION ENDS  -->

    <!-- FOOTER SECTION  -->
    <footer>
        <div class="footer">
            <div class="footer-div">
                <h4>Contact</h4>
                <p><strong>Address:</strong> 30, Vaishnav Vihar, Jhotwara, Jaipur</p>
                <p><strong>Phone:</strong> +916378952948</p>
                <p><strong>Hours:</strong> 24*7</p>
                <h4>Follow Us</h4>
                <a href="#"><i class="fa-brands fa-facebook"></i></a>
                <a href="#"><i class="fa-brands fa-square-instagram"></i></a>
                <a href="#"><i class="fa-brands fa-linkedin"></i></a>
            </div>
            <div class="footer-div links">
                <h4>About</h4>
                <a href="#">
                    About us
                </a>
                <a href="#">
                    Delivery Information
                </a>
                <a href="#">
                    Privacy Policy
                </a>
                <a href="#">
                    Terms & Condition
                </a>
                <a href="#">
                    Contact Us
                </a>
            </div>
            <div class="footer-div links">
                <h4>My Account</h4>
                <a href="#">
                    Sign in
                </a>
                <a href="#">
                    View Cart
                </a>
                <a href="#">
                    My Wishlist
                </a>
                <a href="#">
                    Track My Order
                </a>
                <a href="#">
                    Help
                </a>
            </div>
            <div class="footer-div">
                <p>Secure Payment With </p>
                <img src="/images/1-removebg-preview.png">
                <p>Shipping Partners </p>
                <img src="/images/2-removebg-preview.png">
            </div>
        </div>
    </footer>
    <!-- FOOTER SECTION ENDS  -->

    <!-- COPYRIGHT SECTION  -->
    <div class="copyright">
        <p>&copy; 2023 Developed By Rajeev Prajapat</p>
    </div>
    <!-- COPYRIGHT SECTION ENDS  -->

    <!-- custom Javascript file link -->
    <script src="/js/app.js"></script>
